{
  "presets": [
    [
      "@babel/env",
      {
        "targets": "> 30%, not dead"
      }
    ],
    "@babel/typescript",
    "@babel/react"
  ],
  "plugins": [
    "macros",
    ["styled-components-px2rem", { "rootValue": 100, "unitPrecision": 5, "minPixelValue": 0, "multiplier": 1, "transformRuntime": false }],
    "styled-components"
  ]
}
