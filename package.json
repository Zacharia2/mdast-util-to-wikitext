{
  "name": "md-to-tid",
  "version": "0.0.4",
  "description": "Transform markdown to tiddlywiki5 wikitext syntax.",
  "main": "dist/index.mjs",
  "type": "module",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "build": "npm run build:types && npm run build:js",
    "test": "npm run build:js && cross-env NODE_OPTIONS=--experimental-vm-modules jest --config=jest.config.json",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "rollup -c",
    "prepublishOnly": "npm run clean && npm run build",
    "clean": "rimraf dist public-dist",
    "installType": "typesync"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tiddly-gittly/md-to-tid.git"
  },
  "files": [
    "dist/index.mjs",
    "dist/**/*.d.ts",
    "README.md"
  ],
  "keywords": [
    "generator",
    "template"
  ],
  "author": "lin onetwo",
  "license": " GPL-3.0",
  "bugs": {
    "url": "https://github.com/tiddly-gittly/md-to-tid/issues"
  },
  "homepage": "https://github.com/tiddly-gittly/md-to-tid#readme",
  "dependencies": {
    "@types/mdast": "^4.0.4",
    "@types/unist": "^3.0.3",
    "longest-streak": "^3.0.1",
    "mdast-util-from-markdown": "^2.0.2",
    "mdast-util-phrasing": "^4.1.0",
    "mdast-util-to-string": "^4.0.0",
    "micromark-extension-gfm": "^3.0.0",
    "micromark-util-classify-character": "^2.0.1",
    "micromark-util-decode-string": "^2.0.1",
    "type-fest": "^2.12.2",
    "unist-util-visit": "^5.0.0",
    "zwitch": "^2.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@babel/plugin-transform-modules-commonjs": "^7.17.9",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@blueprintjs/core": "^5.17.5",
    "@fluentui/react": "^8.67.2",
    "@rjsf/core": "^5.24.7",
    "@rjsf/fluent-ui": "^5.24.7",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/babel-plugin-macros": "^2.8.5",
    "@types/babel__core": "^7.1.19",
    "@types/babel__preset-env": "^7.9.2",
    "@types/jest": "^27.5.0",
    "@types/json-schema": "^7.0.15",
    "@types/prettier": "^2.6.0",
    "@types/preval.macro": "^3.0.0",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@types/styled-components": "^5.1.25",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-styled-components": "^1.13.3",
    "babel-plugin-styled-components-px2rem": "^1.5.5",
    "beautiful-react-hooks": "^1.2.0",
    "cross-env": "^7.0.3",
    "jest": "^29.2.6",
    "prettier": "^2.6.2",
    "preval.macro": "^5.0.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-markdown": "^8.0.3",
    "rimraf": "^6.0.1",
    "rollup": "^2.72.0",
    "rollup-jest": "^1.1.3",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-serve": "^1.1.0",
    "rxjs": "^7.5.5",
    "styled-components": "^5.3.5",
    "typescript": "5.8.2",
    "typesync": "0.14.1",
    "use-query-string": "^2.4.1"
  }
}
